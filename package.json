{
  "name": "@vlsergey/js-config",
  "version": "0.0.2",
  "description": "Common configuration files for JavaScript projects",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production babel -d lib src",
    "eslint:check": "eslint --ignore-pattern \"!.eslintrc.js\" ./*.js src/**/*.js test/**/*.tsx",
    "eslint:fix": "eslint --ignore-pattern \"!.eslintrc.js\" --fix ./*.js src/**/*.js test/**/*.tsx",
    "test": "echo \"Error: no test specified\" && exit 1",
    "preversion": "npm run eslint:fix && npm run build",
    "version": "npm publish --access=public",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vlsergey/js-config.git"
  },
  "keywords": [
    "babel",
    "eslint",
    "vlsergey",
    "rules",
    "defaults",
    "configuration"
  ],
  "author": "Sergey Vladimirov <vlsergey@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vlsergey/js-config/issues"
  },
  "homepage": "https://github.com/vlsergey/js-config#readme",
  "dependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/eslint-parser": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.12.13",
    "@types/react": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "autobind-decorator": "^2.4.0",
    "core-js": "^3.9.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.22.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.23.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "regenerator-runtime": "^0.13.7",
    "typescript": "^4.2.3"
  }
}
